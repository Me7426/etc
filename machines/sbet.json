{
  "vars": {
    "root": "~/.xiaket/etc"
  },
  "dirs": [
    "~/.ssh/sockets",
    "${root}/sockets",
    "~/.xiaket/share/github",
    "~/.xiaket/share/stash",
    "~/.xiaket/sbet/bin",
    "~/.xiaket/sbet/etc"
  ],
  "links": [
    {
      "src": "${root}/bashrc",
      "dst": "~/.bashrc"
    },
    {
      "src": "${root}/gitconfig",
      "dst": "~/.gitconfig"
    },
    {
      "src": "${root}/inputrc",
      "dst": "~/.inputrc"
    },
    {
      "src": "${root}/karabiner",
      "dst": "~/.config/karabiner"
    },
    {
      "src": "${root}/pythonrc",
      "dst": "~/.pythonrc"
    },
    {
      "src": "${root}/ptpython",
      "dst": "~/.ptpython"
    },
    {
      "src": "${root}/snape.json",
      "dst": "~/.snape.json"
    },
    {
      "src": "${root}/vim",
      "dst": ["~/.config/nvim", "~/.vim"]
    },
    {
      "src": "~/.vim/init.vim",
      "dst": "~/.vimrc"
    },
    {
      "src": "~/.xiaket/share/github",
      "dst": "~/.Github"
    },
    {
      "src": "~/.xiaket/share/stash",
      "dst": "~/.Stash"
    },
    {
      "src": "~/.xiaket/sbet",
      "dst": "~/.xiaket/alt"
    },
    {
      "src": "~/.xiaket/share/aws",
      "dst": "~/.aws"
    },
    {
      "src": "${root}/pylintrc",
      "dst": "~/.pylintrc"
    },
    {
      "src": "${root}/pylintrc",
      "dst": "~/.pylintrc"
    },
    {
      "src": "~/.xiaket/ssh",
      "dst": "~/.ssh"
    }
  ],
  "builds": [
    {
      "src": "${root}/go/my_prompt.go",
      "lang": "go",
      "dst": "${root}/bin/my_prompt"
    }
  ]
}
