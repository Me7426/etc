{
  "vars": {
    "root": "~/.xiaket/etc"
  },
  "dirs": [
    "~/.ssh/sockets",
    "${root}/sockets"
  ],
  "links": [
    {
      "src": "${root}/bashrc",
      "dst": "~/.bashrc"
    },
    {
      "src": "${root}/gitconfig",
      "dst": "~/.gitconfig"
    },
    {
      "src": "${root}/inputrc",
      "dst": "~/.inputrc"
    },
    {
      "src": "${root}/karabiner",
      "dst": "~/.config/karabiner"
    },
    {
      "src": "${root}/pythonrc",
      "dst": "~/.pythonrc"
    },
    {
      "src": "${root}/ptpython",
      "dst": "~/.ptpython"
    },
    {
      "src": "${root}/snape.json",
      "dst": "~/.snape.json"
    },
    {
      "src": "${root}/vim",
      "dst": ["~/.config/nvim", "~/.vim"]
    },
    {
      "src": "~/.config/nvim/init.vim",
      "dst": "~/.vimrc"
    },
    {
      "src": "~/.xiaket/shared/github",
      "dst": "~/.Github"
    },
    {
      "src": "~/.xiaket/shared/ssh",
      "dst": "~/.ssh"
    }
  ],
  "builds": [
    {
      "src": "${root}/go/my_prompt.go",
      "lang": "go",
      "dst": "${root}/bin/my_prompt"
    }
  ]
}
